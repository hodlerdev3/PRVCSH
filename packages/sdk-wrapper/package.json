{
  "name": "@prvcsh/sdk-wrapper",
  "version": "0.0.1",
  "private": true,
  "description": "Browser-compatible wrapper for PRVCSH SDK with ZK proof generation",
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "import": "./src/types/index.ts"
    },
    "./hooks": {
      "types": "./src/hooks/index.ts",
      "import": "./src/hooks/index.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "import": "./src/utils/index.ts"
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/bn.js": "^5.2.0",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "eslint": "^9.39.1",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "keywords": [
    "solana",
    "privacy",
    "zk-proofs",
    "mixer",
    "sdk"
  ],
  "dependencies": {
    "@lightprotocol/hasher.rs": "^0.2.1",
    "bn.js": "^5.2.2",
    "privacycash": "^1.1.12"
  }
}

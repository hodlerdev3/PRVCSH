{
  "name": "@prvcsh/ui",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.tsx",
      "import": "./src/*.tsx",
      "require": "./src/*.tsx"
    },
    "./styles/*": "./styles/*"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist .turbo",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "^9.39.1",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
